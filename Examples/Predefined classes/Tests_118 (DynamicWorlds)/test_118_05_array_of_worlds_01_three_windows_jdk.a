import 'WorldArray' as 'WindowArray'
	from "Arrays"('TargetClass':='Report');

class 'Main' (specialized 'Alpha'):
--
w	= 'WindowArray'[..] of ('Report');
--
[
goal:-
	w[1] ? writeln("One"),
	w[2] ? writeln("Two"),
	w[3] ? writeln("Three"),!.
]
