class 'Main' (specialized 'Report'):
--
constant:
--
	x	= 'centered';
	y	= 10;
	height	= 10;
	width	= 50;
--
internal:
--
	con	= ('Console');
--
[
goal:-!,
	con ? show,
	writeln("Hello"," ","World","!"),
	writeln(100),
	writeln(100.0),
	writeln('symbol'),
	writeln(structure(1,3.0,"string",'symbol')),
	writeln([1,3.0,"string",'symbol']),
	writeln({a:1,b:3.0,c:"string",d:'symbol'}),
	Text== ?get_string(),
	con ? writeln(Text).
]
