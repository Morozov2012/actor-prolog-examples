class 'Main' (specialized 'Text'):
--
internal:
--
con	= ('Console');
--
[
goal:-!,
	set_string("1234567890"),
	L1== ?length(),
	con ? writeln("Length 1: ",L1).
--
get_string()= "123":-!.
]
