--
PREDICATES:
--
imperative:
--
test0;
test1;
--
-----------------------------------------------------------------------
class 'Main' (specialized 'WebResource'):
--
constant:
--
	location= "data.dir/report1.html";
--
internal:
--
	con	= ('Console');
--
[
goal:-!,
	test0,
	test1.
--
test0:-
	?get_string().
--
test1:-
	Content== ?get_string(),
	con ? writeln(Content).
]
-----------------------------------------------------------------------
