-----------------------------------------------------------------------
--
PREDICATES:
--
imperative:
--
test1;
test2;
--
-----------------------------------------------------------------------
class 'MyHashSet' (specialized 'HashSet'):
[
DOMAINS:
--
Target	= f(N,R,S).
N	= INTEGER.
R	= REAL.
S	= STRING.
]
-----------------------------------------------------------------------
class 'Main' (specialized 'Console'):
--
internal:
--
store	= ('MyHashSet');
--
[
goal:-!,
	test1,
	test2.
--
test1:-
	writeln("I will store data items."),
	store ? append(f(1000,3.14,"Text1")),
	store ? append(f(1000,7.0,"Text1")),
	store ? append(f(1000,3.14,"Text1")),
	store ? append(f(3000,3.14,"Text1")),
	store ? append(f(1000,3.14,"Text1")),
	store ? append(f(1000,3.14,"Text2")),
	store ? append(f(5000,9.00,"Text3")),
	writeln("Done.").
--
test2:-
	writeln("I will retrieve data items:"),
	store ? find(Item),
	writeln(Item),
	fail.
test2:-
	writeln("Done.").
]
-----------------------------------------------------------------------
