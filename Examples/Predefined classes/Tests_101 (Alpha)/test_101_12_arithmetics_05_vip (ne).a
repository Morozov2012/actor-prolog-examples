	A== "abcdefg",
	B== "hij",
	con ? writeln("   A=",A," (a string)"),
	con ? writeln("   B=",B," (a string)"),
	B <> A,!,
	con ? writeln("   O.K.: B <> A").
test1:-
	con ? writeln("Failure: B = A").
