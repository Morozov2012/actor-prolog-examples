variable:
--
	list2	: List;
--
[
PREDICATES:

determ:

'<>'(RoundTime,RoundTime)	- (i,i);
'<>'(Time,RoundTime)		- (i,i);
'<>'(RoundTime,Time)		- (i,i);
'<>'(List,List)			- (i,i);

