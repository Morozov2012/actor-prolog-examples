--
	world23	: D3;
	world24	: D3;
--
	world34	: D4;
	world35	: D4;
	world36	: D4;
--
[
PREDICATES:
--
imperative:
--
set_world23(D3)		- (i);
set_world24(D3)		- (i);
--
set_world34(D4)		- (i);
set_world35(D4)		- (i);
set_world36(D4)		- (i);
--
CLAUSES:
--
