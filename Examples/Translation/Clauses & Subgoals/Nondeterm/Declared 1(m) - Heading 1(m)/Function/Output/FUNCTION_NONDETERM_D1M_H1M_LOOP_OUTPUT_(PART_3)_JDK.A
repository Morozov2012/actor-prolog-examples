	world1	: 'Testbed'	= self;
--
variable:
--
	world21	: D3		= self;
	world22	: D3		= #;
	world23	: D3		= _;
	world24	: D3		= _;
--
	world31	: D4		= self;
	world32	: D4		= #;
	world33	: D4		= -100;
	world34	: D4		= _;
	world35	: D4		= _;
	world36	: D4		= _;
--
internal:
--
	auxiliary_tools		= ('AuxiliaryTools',
					world23,
					world24,
					world34,
					world35,
					world36);
--
	con			= ('Console');
--
[
PREDICATES:
--
nondeterm:
--
loop_D1(D1*)		- (o);
loop_D2(D4*)		- (o);
--
p(D1*) = STRING		- (o);
q(D4*) = STRING		- (o);
--
r(D4*) = R_STRING	- (o)(i);
r(D4,D4*) = R_STRING	- (i,o)(i,i);
--
CLAUSES:
--
goal:-
	--
	loop_D1(N1001,N0101,N0102,N0103),
	con ? writeln("N1001: ",N1001," N0101: ",N0101," N0102: ",N0102," N0103: ",N0103),
	--
	fail.
goal:-
	--
	loop_D2(N1002,N0201,N0202,N0203),
	con ? writeln("N1002: ",N1002," N0201: ",N0201," N0202: ",N0202," N0203: ",N0203),
	--
	fail.
goal:-
	con ? writeln("O.K.").
--
alarm(_):-
	fail.
]
