-------------------------------------------
--                                       --
-- Элементы структурного логического     --
-- программирования.                     --
--                                       --
-- Собрать и вернуть список.             --
--                                       --
-- Использованы "кирпичики":             --
-- 1) входная сборка списка;             --
-- 2) выходной аргумент.                 --
--                                       --
-------------------------------------------

DOMAINS:

List	= INTEGER*.

PREDICATES:

nondeterm:

p(List,List)		- (i,o);

CLAUSES:

goal:-
	p([],Y),
	writeln(Y).

p(Хвост,Результат):-
	Голова== ?input_integer(
		"Введите целое число",0),
	p([Голова|Хвост],Результат).
p(СобранныйСписок,СобранныйСписок).
