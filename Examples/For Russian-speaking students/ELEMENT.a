DOMAINS:

reference:

N	= INTEGER.
List	= N*.

PREDICATES:

nondeterm:

element(N,List)		- (_,_);

CLAUSES:

goal:-
	element(3,[1,2,3,4,5]),
	writeln("Yes").
goal:-
	writeln("No").

element(H,[H|_]).
element(H,[_|Rest]):-
	element(H,Rest).
