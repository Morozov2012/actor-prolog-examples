------------------------------------------------------------------------
-- Пример программы на Акторном Прологе.                              --
-- Небольшая визуальная экспертная система.                           --
-- Алексей А. Морозов, ИРЭ РАН.                                       --
-- 11 декабря 1998 г.                                                 --
------------------------------------------------------------------------
--
DOMAINS:
--
reference:
--
ПараметрыМесторождения	= {	rock: ГорнаяПорода,
				stratum: Пласт,
				oil: ПластоваяНефть,
				water: ПластоваяВода,
				gas: ПластовыйГаз,
				substances: ПластовыеВещества,
				composition: МинеральныйСостав}.
--
ГорнаяПорода		= {	type_of_rock: ТипПороды,
				type_of_collector: ТипКоллектора,
				penetrability: Проницаемость,
				porosity: Пористость,
				saturation: Нефтенасыщенность,
				water: СвязаннаяВода,
				moist: СреднийУголСмачивания}.
--
ТипПороды		= "терригенный"; "карбонатный".
ТипКоллектора		= "поровый"; "трещинный"; "смешанный".
Проницаемость		= REAL; STRING.
Пористость		= REAL; STRING.
Нефтенасыщенность	= REAL; STRING.
СвязаннаяВода		= REAL; STRING.
СреднийУголСмачивания	= NUMERICAL; STRING. -- [0 .. 180]
--
Пласт		= {	thickness: ТолщинаПласта,
			water_thickness: ТолщинаВодонасыщеннойЗоны,
			rock_thickness: ТолщинаПокрывающихПород,
			pressure: Давление,
			temperature: Температура,
			angle: УголПадения,
			depth: ГлубинаЗалегания}.
--
ТолщинаПласта			= NUMERICAL; STRING.
ТолщинаВодонасыщеннойЗоны	= REAL; STRING.
ТолщинаПокрывающихПород		= REAL; STRING.
Давление			= REAL; STRING.
Температура			= NUMERICAL; STRING.
УголПадения			= NUMERICAL; STRING. -- [0 .. 90]
ГлубинаЗалегания		= NUMERICAL; STRING.
--
ПластоваяНефть		= {	density: Плотность,
				viscosity: Вязкость,
				acid: КислотноеЧисло}.
--
Плотность		= NUMERICAL; STRING.
Вязкость		= REAL; STRING.
КислотноеЧисло		= REAL; STRING.
--
ПластоваяВода		= {	mineralization: Минерализация,
				pH: Кислотность,
				hardness: Жёсткость}.
--
Минерализация		= REAL; STRING.
Кислотность		= REAL; STRING.
Жёсткость		= REAL; STRING.
--
ПластовыйГаз		= {	nitrogen: АзотистыеСоединения,
				free_gas: НаличиеСвободногоГаза}.
--
АзотистыеСоединения	= REAL; STRING.
НаличиеСвободногоГаза	= 'yes'; 'no'; 'unknown'.
--
ПластовыеВещества	= {	paraph: Парафин,
				asphalt: Асфальтены,
				resin: Смолы,
				sulphur: Сера,
				t_paraph: ТемператураНасыщения}.
--
Парафин			= REAL; STRING.
Асфальтены		= REAL; STRING.
Смолы			= REAL; STRING.
Сера			= REAL; STRING.
ТемператураНасыщения	= REAL; STRING.
--
МинеральныйСостав	= {	cement: Цемент,
				clay: Глины,
				carbon: Карбонатность}.
--
Цемент			= REAL; STRING.
Глины			= REAL; STRING.
Карбонатность		= REAL; STRING.
--
MethodNames		= MethodName*.
MethodName		= STRING; MethodNames.
--
NUMERICAL		= REAL; INTEGER.
--
ground:
--
GroundNumber		= REAL; INTEGER.
ИмяПараметра		= STRING.
Tabulation		= INTEGER.
--
