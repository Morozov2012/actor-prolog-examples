------------------------------------------------------------------------
-- Базовый класс 'Проверка_параметров'                                --
------------------------------------------------------------------------
interface 'Проверка_параметров'
(specialized '_Block','MethodNameProducer'):
--
variable:
--
	value_i1	: ПараметрыМесторождения;
--
protecting:
--
	value_o1	: MethodName;
--
constant:
--
	name		: MethodName;
--
internal:
--
	dialog		: 'ДиалогПроверкиПараметров';
	tools		: 'ArithmeticOperations';
--
constant:
--
	controller	: 'MethodNameSender';
--
[
PREDICATES:
--
private:
--
determ:
--
parameters_are_not_valid;
--
visible:
--
imperative:
--
verify_parameters;
--
private:
--
determ:
--
'<'(Проницаемость,Проницаемость)		- (i,i);
'<'(	ТолщинаПокрывающихПород,
	ТолщинаПокрывающихПород)		- (i,i);
'<'(	ТемператураНасыщения,
	Температура)				- (i,i);
--
'<='(Проницаемость,Проницаемость)		- (i,i);
'<='(Пористость,Пористость)			- (i,i);
'<='(Нефтенасыщенность,Нефтенасыщенность)	- (i,i);
'<='(СвязаннаяВода,СвязаннаяВода)		- (i,i);
'<='(	СреднийУголСмачивания,
	СреднийУголСмачивания)			- (i,i);
'<='(ТолщинаПласта,ТолщинаПласта)		- (i,i);
'<='(	ТолщинаВодонасыщеннойЗоны,
	ТолщинаВодонасыщеннойЗоны)		- (i,i);
'<='(	ТолщинаПокрывающихПород,
	ТолщинаПокрывающихПород)		- (i,i);
'<='(Давление,Давление)				- (i,i);
'<='(Температура,Температура)			- (i,i);
'<='(УголПадения,УголПадения)			- (i,i);
'<='(ГлубинаЗалегания,ГлубинаЗалегания)		- (i,i);
'<='(Плотность,Плотность)			- (i,i);
'<='(Вязкость,Вязкость)				- (i,i);
'<='(КислотноеЧисло,КислотноеЧисло)		- (i,i);
'<='(Минерализация,Минерализация)		- (i,i);
'<='(Кислотность,Кислотность)			- (i,i);
'<='(Жёсткость,Жёсткость)			- (i,i);
'<='(АзотистыеСоединения,АзотистыеСоединения)	- (i,i);
'<='(Парафин,Парафин)				- (i,i);
