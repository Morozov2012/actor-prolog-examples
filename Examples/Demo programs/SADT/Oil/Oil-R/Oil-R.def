-- Базовый класс 'Способы_воздействия'                                --
------------------------------------------------------------------------
interface 'Способы_воздействия' (specialized 'Объяснение_решений'):
[
PREDICATES:
--
private:
--
determ:
--
check_parameters(ПараметрыМесторождения)	- (i);
check_rock(ГорнаяПорода)			- (i);
--
check_type_of_rock(ТипПороды)			- (i);
check_type_of_collector(ТипКоллектора)		- (i);
check_penetrability(Проницаемость)		- (i);
check_porosity(Пористость)			- (i);
check_saturation(Нефтенасыщенность)		- (i);
check_collected_water(СвязаннаяВода)		- (i);
check_moist(СреднийУголСмачивания)		- (i);
--
check_stratum(Пласт)				- (i);
--
check_thickness(ТолщинаПласта)				- (i);
check_water_thickness(ТолщинаВодонасыщеннойЗоны)	- (i);
check_rock_thickness(ТолщинаПокрывающихПород)		- (i);
check_pressure(Давление)				- (i);
check_temperature(Температура)				- (i);
check_angle(УголПадения)				- (i);
check_depth(ГлубинаЗалегания)				- (i);
--
check_oil(ПластоваяНефть)			- (i);
--
check_density(Плотность)			- (i);
check_viscosity(Вязкость)			- (i);
check_acid(КислотноеЧисло)			- (i);
--
check_water(ПластоваяВода)			- (i);
--
check_mineralization(Минерализация)		- (i);
check_pH(Кислотность)				- (i);
check_hardness(Жёсткость)			- (i);
--
check_gas(ПластовыйГаз)				- (i);
--
check_nitrogen(АзотистыеСоединения)		- (i);
check_free_gas(НаличиеСвободногоГаза)		- (i);
--
check_substances(ПластовыеВещества,Пласт)	- (i,i);
--
check_paraph(Парафин)				- (i);
check_asphalt(Асфальтены)			- (i);
check_resin(Смолы)				- (i);
check_sulphur(Сера)				- (i);
check_T_paraph(
	ТемператураНасыщения,
	Температура)				- (i,i);
--
check_composition(МинеральныйСостав)		- (i);
--
check_cement(Цемент)				- (i);
check_clay(Глины)				- (i);
check_carbon(Карбонатность)			- (i);
--
determ:
--
type_of_rock(ТипПороды)					- (i);
type_of_collector(ТипКоллектора)			- (i);
penetrability(Проницаемость,Проницаемость)		- (o,o);
porosity(Пористость,Пористость)				- (o,o);
saturation(Нефтенасыщенность,Нефтенасыщенность)		- (o,o);
collected_water(СвязаннаяВода,СвязаннаяВода)		- (o,o);
moist(СреднийУголСмачивания,СреднийУголСмачивания)	- (o,o);
--
thickness(ТолщинаПласта,ТолщинаПласта)			- (o,o);
water_thickness(
	ТолщинаВодонасыщеннойЗоны,
	ТолщинаВодонасыщеннойЗоны)			- (o,o);
rock_thickness(
	ТолщинаПокрывающихПород,
	ТолщинаПокрывающихПород)			- (o,o);
pressure(Давление,Давление)				- (o,o);
temperature(Температура,Температура)			- (o,o);
angle(УголПадения,УголПадения)				- (o,o);
depth(ГлубинаЗалегания,ГлубинаЗалегания)		- (o,o);
--
density(Плотность,Плотность)				- (o,o);
viscosity(Вязкость,Вязкость)				- (o,o);
acid(КислотноеЧисло,КислотноеЧисло)			- (o,o);
--
mineralization(Минерализация,Минерализация)		- (o,o);
pH(Кислотность,Кислотность)				- (o,o);
hardness(Жёсткость,Жёсткость)				- (o,o);
--
nitrogen(АзотистыеСоединения,АзотистыеСоединения)	- (o,o);
free_gas(НаличиеСвободногоГаза)				- (i);
--
paraph(Парафин,Парафин)					- (o,o);
asphalt(Асфальтены,Асфальтены)				- (o,o);
resin(Смолы,Смолы)					- (o,o);
sulphur(Сера,Сера)					- (o,o);
--
cement(Цемент,Цемент)					- (o,o);
clay(Глины,Глины)					- (o,o);
carbon(Карбонатность,Карбонатность)			- (o,o);
]
