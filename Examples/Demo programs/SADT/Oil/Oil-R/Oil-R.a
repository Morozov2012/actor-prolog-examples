	con ? write(Text2),
	con ? set_color('default'),
	con ? writeln(".").
--
is_unused(Text1):-!,
	is_unused(0,Text1).
--
is_unused(N,Text1):-!,
	is_unused(N,Text1,"не учитывается").
is_unused(N,Text1,Text2):-
	write_tab(N),
	con ? write(Text1),
	con ? write(" - "),
	con ? set_color('Blue'),
	con ? write(Text2),
	con ? set_color('default'),
	con ? writeln(".").
--
write_tab(N):-
	tools?(N <= 0),!.
write_tab(N):-
	con ? write("   "),
	N2 == tools?'-'(N,1),
	write_tab(N2).
------------------------------------------------------------------------
method_is_unknown:-
	con ? clear,
	con ? show,
	con ? set_color('Red'),
	con ? writeln("Метод воздействия: ",name),
	con ? nl,
	con ? writeln(
		"Извините, про этот метод мне\n",
		"просто ничего не известно."),
	con ? set_color('default').
]
