			stratum: ST,
			oil: O,
			water: W,
			gas: G,
			substances: SS,
			composition: C
			|_}):-!,
	con ? clear,
	con ? show,
	con ? set_color('Blue'),
	con ? writeln("The name of method: ",name),
	con ? nl,
	con ? set_color('default'),
	con ? writeln("Parameters check:"),
	verify_rock(R,"Rock"),
	verify_stratum(ST,"Bed"),
	verify_oil(O,"Base oil"),
	verify_water(W,"Stratal water"),
	verify_gas(G,"Native gas"),
	verify_substances(SS,ST,"Bedded substances"),
	verify_composition(C,"Mineral constituents"),
	con ? writeln("Check is complete.").
verify_parameters(P):-!,
	con ? clear,
	con ? show,
	con ? set_color('Red'),
	con ? writeln("Name of method: ",name),
	con ? nl,
	con ? writeln("Parameters are not assigned!"),
	con ? set_color('default'),
	con ? writeln(P).
