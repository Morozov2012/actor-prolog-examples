------------------------------------------------------------------------
-- General class 'Methods_of_influence'                               --
------------------------------------------------------------------------
class 'Methods_of_influence' (specialized 'Explanation_of_solutions'):
[
check_parameters({	rock: R,
			stratum: ST,
			oil: O,
			water: W,
			gas: G,
			substances: SS,
			composition: C
			|_}):-
	check_rock(R),
	check_stratum(ST),
	check_oil(O),
	check_water(W),
	check_gas(G),
	check_substances(SS,ST),
	check_composition(C).
------------------------------------------------------------------------
check_rock({	type_of_rock: RT,
		type_of_collector: CT,
		penetrability: PN,
		porosity: PR,
		saturation: S,
		water: W,
		moist: M
		|_}):-
	check_type_of_rock(RT),
	check_type_of_collector(CT),
	check_penetrability(PN),
	check_porosity(PR),
	check_saturation(S),
	check_collected_water(W),
	check_moist(M).
--
check_type_of_rock(RT):-!,
	type_of_rock(RT),!.
check_type_of_collector(CT):-!,
	type_of_collector(CT),!.
check_penetrability(PN):-
	penetrability(MinPenetrability,MaxPenetrability),!,
	--
	MinPenetrability <= PN,
	PN <= MaxPenetrability.
check_porosity(PR):-
	porosity(MinPorosity,MaxPorosity),!,
	--
	MinPorosity <= PR,
	PR <= MaxPorosity.
check_saturation(S):-
	saturation(MinSaturation,MaxSaturation),!,
	--
	MinSaturation <= S,
	S <= MaxSaturation.
check_collected_water(W):-
	collected_water(MinWater,MaxWater),!,
	--
	MinWater <= W,
	W <= MaxWater.
check_moist(M):-
	moist(MinMoist,MaxMoist),!,
	--
	MinMoist <= M,
	M <= MaxMoist.
------------------------------------------------------------------------
check_stratum({	thickness: TH,
		water_thickness: WT,
		rock_thickness: RT,
		pressure: P,
		temperature: T,
		angle: A,
		depth: D
		|_}):-
	check_thickness(TH),
	check_water_thickness(WT),
	check_rock_thickness(RT),
	check_pressure(P),
	check_temperature(T),
	check_angle(A),
	check_depth(D).
--
check_thickness(TH):-
	thickness(MinThickness,MaxThickness),!,
	--
	MinThickness <= TH,
	TH <= MaxThickness.
check_water_thickness(WT):-
	water_thickness(MinWaterThickness,MaxWaterThickness),!,
	--
	MinWaterThickness <= WT,
	WT <= MaxWaterThickness.
check_rock_thickness(RT):-
	rock_thickness(MinRockThickness,MaxRockThickness),!,
	--
	MinRockThickness <= RT,
	RT <= MaxRockThickness.
check_pressure(P):-
	pressure(MinPressure,MaxPressure),!,
	--
	MinPressure <= P,
	P <= MaxPressure.
check_temperature(T):-
	temperature(MinTemperature,MaxTemperature),!,
	--
	MinTemperature <= T,
	T <= MaxTemperature.
check_angle(A):-
	angle(MinAngle,MaxAngle),!,
	--
	MinAngle <= A,
	A <= MaxAngle.
check_depth(D):-
	depth(MinDepth,MaxDepth),!,
	--
	MinDepth <= D,
	D <= MaxDepth.
------------------------------------------------------------------------
check_oil({	density: D,
		viscosity: V,
		acid: A
		|_}):-
	check_density(D),
	check_viscosity(V),
	check_acid(A).
--
check_density(D):-
	density(MinDensity,MaxDensity),!,
	--
	MinDensity <= D,
	D <= MaxDensity.
check_viscosity(V):-
	viscosity(MinViscosity,MaxViscosity),!,
	--
	MinViscosity <= V,
	V <= MaxViscosity.
check_acid(A):-
	acid(MinAcid,MaxAcid),!,
	--
	MinAcid <= A,
	A <= MaxAcid.
------------------------------------------------------------------------
check_water({	mineralization: M,
		pH: PH,
		hardness: H
		|_}):-
	check_mineralization(M),
	check_pH(PH),
	check_hardness(H).
--
check_mineralization(M):-
	mineralization(MinMineralization,MaxMineralization),!,
	--
	MinMineralization <= M,
	M <= MaxMineralization.
check_pH(PH):-
	pH(Min_pH,Max_pH),!,
	--
	Min_pH <= PH,
	PH <= Max_pH.
check_hardness(H):-
	hardness(MinHardness,MaxHardness),!,
	--
	MinHardness <= H,
	H <= MaxHardness.
------------------------------------------------------------------------
check_gas({nitrogen:N,free_gas:FG|_}):-!,
	check_nitrogen(N),
	check_free_gas(FG).
--
check_nitrogen(N):-
	nitrogen(MinNitrogen,MaxNitrogen),!,
	--
	MinNitrogen <= N,
	N <= MaxNitrogen.
--
check_free_gas(FG):-
	free_gas(FG),!.
------------------------------------------------------------------------
check_substances({
		paraph: P,
		asphalt: A,
		resin: R,
		sulphur: S,
		t_paraph: TO
		|_},
		{
		temperature: TS
		|_}):-
	check_paraph(P),
	check_asphalt(A),
	check_resin(R),
	check_sulphur(S),
	check_T_paraph(TO,TS).
--
check_paraph(P):-
	paraph(MinParaph,MaxParaph),!,
	--
	MinParaph <= P,
	P <= MaxParaph.
check_asphalt(A):-
	asphalt(MinAsphalt,MaxAsphalt),!,
	--
	MinAsphalt <= A,
	A <= MaxAsphalt.
check_resin(R):-
	resin(MinResin,MaxResin),!,
	--
	MinResin <= R,
	R <= MaxResin.
check_sulphur(S):-
	sulphur(MinSulphur,MaxSulphur),
	--
	MinSulphur <= S,
	S <= MaxSulphur.
------------------------------------------------------------------------
check_composition({cement:C1,clay:C2,carbon:C3|_}):-!,
	check_cement(C1),
	check_clay(C2),
	check_carbon(C3),!.
--
check_cement(C1):-
	cement(MinCement,MaxCement),!,
	--
	MinCement <= C1,
	C1 <= MaxCement.
check_clay(C2):-
	clay(MinClay,MaxClay),!,
	--
	MinClay <= C2,
	C2 <= MaxClay.
check_carbon(C3):-
	carbon(MinCarbon,MaxCarbon),!,
	--
	MinCarbon <= C3,
	C3 <= MaxCarbon.
------------------------------------------------------------------------
type_of_rock(_):-
	fail.
type_of_collector(_):-
	fail.
penetrability(#,#).
porosity(#,#).
saturation(#,#).
collected_water(#,#).
moist(#,#).
--
thickness(#,#).
water_thickness(#,#).
rock_thickness(#,#).
pressure(#,#).
temperature(#,#).
angle(#,#).
depth(#,#).
--
density(#,#).
viscosity(#,#).
acid(#,#).
--
mineralization(#,#).
pH(#,#).
hardness(#,#).
--
nitrogen(#,#).
free_gas(_):-
	fail.
--
paraph(#,#).
asphalt(#,#).
resin(#,#).
sulphur(#,#).
--
cement(#,#).
clay(#,#).
carbon(#,#).
]
