class '_Assemble_list' (specialized '_Block'):
--
value_i1;
value_i2;
value_i3;
value_i4;
value_i5;
value_i6;
value_i7;
--
value_o1;
--
controller	= ('MethodNameSender',
			target=self,
			output=value_o1);
--
[
goal:-!,
	List1== [	value_i1,value_i2,value_i3,
			value_i4,value_i5,value_i6,value_i7],
	clear_list(List1,List2),
	-- send_value(List2).
	list_is_not_empty(List2).
--
clear_list([],[]):-!.
clear_list([#|Rest1],Rest2):-!,
	clear_list(Rest1,Rest2).
clear_list([Name|Rest1],[Name|Rest2]):-!,
	clear_list(Rest1,Rest2).
clear_list(_,[]).
--
list_is_not_empty([]):-!,
	fail.
list_is_not_empty(_).
--
get_result()= List2 :-
	List1== [	value_i1,value_i2,value_i3,
			value_i4,value_i5,value_i6,value_i7],
	clear_list(List1,List2).
--
show(_,_):-!.
]
