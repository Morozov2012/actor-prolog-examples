-- Artificial Waterflooding                                           --
------------------------------------------------------------------------
class '_Artificial_waterflooding'
(specialized 'Methods_of_influence'):
[
check_collected_water(_):-!.
--
type_of_rock("terrigenous"):-!.
type_of_rock("carbonate"):-!.
type_of_collector("porous"):-!.
penetrability(0.1,5.0):-!.
porosity(0.1,0.5):-!.
saturation(0.7,1.0):-!.
moist(0,90):-!.
--
check_water_thickness(_):-!.
check_pressure(_):-!.
check_depth(_):-!.
--
check_rock_thickness(RT):-!,
	min_rock_thickness(MinRockThickness),!,
	MinRockThickness < RT.
--
thickness(3,100):-!.
min_rock_thickness(3.0):-!.
temperature(20,100):-!.
angle(0,5):-!.
--
check_acid(_):-!.
--
density(650,1000):-!.
viscosity(0.01,25.0):-!.
--
check_water(_):-!.
--
check_gas(_):-!.
--
check_asphalt(_):-!.
check_resin(_):-!.
--
check_T_paraph(TO,TS):-!,
	TO < TS.
--
paraph(0.0,0.055):-!.
sulphur(0.0,0.02):-!.
--
check_cement(_):-!.
check_carbon(_):-!.
--
clay(0.0,0.05):-!.
]
