------------------------------------------------------------------------
is_valid(Text1):-!,
	is_valid(0,Text1).
is_valid(N,Text1):-!,
	is_valid(N,Text1,"quite suited").
is_valid(N,Text1,Text2):-!,
	write_tab(N),
	con ? write(Text1),
	con ? write(" - "),
	con ? set_color('Green'),
	con ? write(Text2),
	con ? set_color('default'),
	con ? writeln(".").
--
is_not_valid(Text1):-!,
	is_not_valid(0,Text1).
is_not_valid(N,Text1):-!,
	is_not_valid(N,Text1,"inadmissible").
is_not_valid(N,Text1,Text2):-!,
	write_tab(N),
	con ? write(Text1),
	con ? write(" - "),
	con ? set_color('Red'),
	con ? write(Text2),
	con ? set_color('default'),
	con ? writeln(".").
--
is_unused(Text1):-!,
	is_unused(0,Text1).
--
is_unused(N,Text1):-!,
	is_unused(N,Text1,"unimportant").
is_unused(N,Text1,Text2):-
	write_tab(N),
	con ? write(Text1),
	con ? write(" - "),
	con ? set_color('Blue'),
	con ? write(Text2),
	con ? set_color('default'),
	con ? writeln(".").
--
write_tab(N):-
	tools?(N <= 0),!.
write_tab(N):-
	con ? write("   "),
	write_tab(tools?'-'(N,1)).
------------------------------------------------------------------------
method_is_unknown:-
	con ? clear,
	con ? show,
	con ? set_color('Red'),
	con ? writeln("Name of method: ",name),
	con ? nl,
	con ? writeln(
		"Sorry, no information\n",
		"about this method."),
	con ? set_color('default').
]
------------------------------------------------------------------------
-- General class 'Check_of_parameters'                                --
------------------------------------------------------------------------
class 'Check_of_parameters' (specialized '_Block'):
--
value_i1;
--
value_o1;
--
title;
name;
x;
y;
--
dialog		= ('ParametersCheckDialog',
			title,
			x,
			y,
			owner=self);
--
tools		= ('ArithmeticOperations');
--
controller	= ('MethodNameSender',
			target=self,
			output=value_o1);
--
[
goal:-!,
	tools ? bound(value_i1),
	check_parameters(value_i1).
--
get_result()= name :-
	check_parameters(value_i1).
--
show(_,_):-
	parameters_are_not_valid,
	verify_parameters(value_i1),
	fail.
show(_,_):-
	dialog ? show,
	fail.
--
parameters_are_not_valid:-
	check_parameters(value_i1),!,
	fail.
parameters_are_not_valid.
--
verify_parameters:-
	verify_parameters(value_i1),
	fail.
verify_parameters.
--
X < Y :-!,
	tools ? '<'(?val("GroundNumber",X),?val("GroundNumber",Y)).
X <= Y :-!,
	tools ? '<='(?val("GroundNumber",X),?val("GroundNumber",Y)).
