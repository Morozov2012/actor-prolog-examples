DOMAINS:
--
N	= INTEGER.
--
-------------------------------------------
interface 'Web1'
(specialized 'WebReceptor'):
--
w1	: 'Report';
w2	: 'Report';
w3	: 'Report';
w4	: 'Report';
--
[
PREDICATES:
--
imperative:
--
write_parameters(ContentParameters);
write_date(ResourceDate);
write_time(ResourceTime);
write_references(N,ReferenceList);
write_tag_structure(N,TreeContent);
shift_text(N);
]
