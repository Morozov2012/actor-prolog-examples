                        }
                      }
                      geometry Box{
                        size 0.032 0.0159 0.0001
                      }
                    }
                  ]
                }
                Group{
                  children[
                    Shape{
                      appearance Appearance{
                        material Material{
                          transparency 1
                        }
                      }
                      geometry IndexedFaceSet{
                        coord Coordinate{
                          point[
                            0 0 0,
                            0.04 0 0,
                            0.04 0.017 0,
                            0 0.018 0,
                            0.09 0.065 0.161
                          ]
                        }
                        coordIndex[
                          0 1 2 3 -1
                          1 4 0 -1
                          2 4 1 -1
                          3 4 2 -1
                          4 3 0 -1
                        ]
                      }
                    }
                    Transform{
                      translation 0.02 0.0085 0
                      children[
                        Shape{
                          appearance Appearance{
                            material Material{
                              emissiveColor 0 1 0
                              diffuseColor 0 1 0
                              specularColor 0 1 0
                              transparency 0.5
                            }
                          }
                          geometry Box{
                            size 0.032 0.0159 0.0001
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
        Shape{
          geometry Text{
            string ["Fly !"]
            fontStyle FontStyle{
              family "SERIF"
              style "BOLD"
              size 0.015
            }
          }
        }
        DEF TS1 TouchSensor{
        }
      ]
    }
  ]
}
DEF TOGGLE Script{
  eventIn SFBool isOver
  eventOut SFInt32 whichChoice
  url "javascript:
    function isOver(value,time){
      if(value==true) whichChoice= 1;
      else whichChoice= 0;
    }"
  }
ROUTE TS1.isOver TO TOGGLE.isOver
ROUTE TOGGLE.whichChoice TO SELECT.whichChoice
ROUTE PS1.orientation_changed TO Dashboard.set_rotation
ROUTE PS1.position_changed TO Dashboard.set_translation
